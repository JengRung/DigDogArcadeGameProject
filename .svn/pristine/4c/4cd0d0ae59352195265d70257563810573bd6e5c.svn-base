import java.io.File;
import java.io.IOException;

import javax.imageio.ImageIO;
/**
 */
public class Fruit extends GameObject{
		
	public Fruit() {
		
		this.Score = 300;		
		
		try{
			this.image = ImageIO.read(new File("f1.png"));
		
	}	 catch(IOException e){
			throw new RuntimeException("Error drawing Fruit: "+e);
	}
	}	
	
}
	
